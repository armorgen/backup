 name: Update packages
      run: |
          cd openwrt
          git clone https://github.com/skywalkker/passwall.git  
          git clone  -b master https://github.com/vernesong/OpenClash.git  
          svn co --force https://github.com/coolsnowwolf/lede/trunk/package/lean/luci-app-ipsec-vpnd package/lean/luci-app-ipsec-vpnd   && svn revert -R package/lean/luci-app-ipsec-vpnd
          svn co --force https://github.com/coolsnowwolf/lede/trunk/package/lean/luci-app-adbyby-plus package/lean/luci-app-adbyby-plus   && svn revert -R package/lean/luci-app-adbyby-plus 
